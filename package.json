    "lab": "^11.2.0",
    "lodash": "^4.16.6",
    "platform": "^1.3.2",
    "prompt": "^1.0.0",
    "proxyquire": "1.4.0",
    "sinon": "1.17.6",
    "sinon-as-promised": "4.0.2",
    "snyk": "1.19.1"
  },
  "snyk": true
}
