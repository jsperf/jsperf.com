    "catbox-redis": "^3.1.1",
    "confidence": "3.0.2",
    "dotenv": "^6.1.0",
    "first-mate": "^7.4.1",
    "glue": "^4.2.0",
    "good": "^7.3.0",
    "good-console": "^6.4.0",
    "good-loggly": "3.1.0",
    "good-squeeze": "^5.1.0",
    "gulp": "3.9.1",
    "handlebars": "^4.0.12",
    "hapi": "^16.6.2",
    "hapi-auth-cookie": "7.0.0",
    "hapi-qs": "1.1.3",
    "highlight.js": "^9.8.0",
    "hoek": "^5.0.4",
    "inert": "^4.2.1",
    "joi": "^13.7.0",
    "jsesc": "^2.3.0",
    "lodash.assign": "4.2.0",
    "lodash.omit": "4.5.0",
