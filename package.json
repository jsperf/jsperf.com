    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "gulp-add-src": "0.2.0",
    "gulp-clean-css": "^3.10.0",
