{
  "name": "jsperf.com",
  "version": "2.0.0",
  "description": "jsPerf aims to provide an easy way to create and share test cases, comparing the performance of different JavaScript snippets by running benchmarks",
  "main": "index.js",
  "scripts": {
    "build": "gulp",
    "clean": "rm npm-debug.log lcov.info coverage.html",
    "lint": "SELENIUM_REMOTE_URL=http://skip JSPERF_REMOTE_URL=http://skip lab -d -L",
    "start": "node server.js",
    "test": "lab -t 100 -c -L -r lcov -o lcov.info -r console -o stdout -r html -o coverage.html test/unit",
    "test-e2e": "lab -L -m 10000 test/e2e",
    "watch": "nodemon --ext js,hbs server.js",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jsperf/jsperf.com.git"
  },
  "keywords": [
    "jsperf"
  ],
  "contributors": [
    "Max Beatty"
  ],
  "bugs": {
    "url": "https://github.com/jsperf/jsperf.com/issues"
  },
  "homepage": "https://github.com/jsperf/jsperf.com",
  "dependencies": {
    "bell": "8.2.0",
    "blipp": "2.3.0",
    "boom": "4.0.0",
    "confidence": "3.0.1",
    "debug": "2.2.0",
    "dotenv": "2.0.0",
    "glue": "4.0.0",
    "good": "7.0.2",
    "good-console": "6.1.2",
    "handlebars": "4.0.5",
    "hapi": "15.0.3",
    "hapi-auth-cookie": "6.1.1",
    "hapi-qs": "1.1.1",
    "highlight.js": "9.6.0",
    "hoek": "4.0.1",
    "inert": "4.0.1",
    "joi": "9.0.4",
    "jsesc": "2.2.0",
    "lodash.assign": "4.2.0",
    "lodash.omit": "4.5.0",
    "marked": "0.3.6",
    "mysql": "2.5.5",
    "scooter": "4.0.0",
    "snyk": "1.19.1",
    "vision": "4.1.0",
    "visionary": "6.0.1",
    "yar": "8.0.0"
  },
  "devDependencies": {
    "benchmark": "2.1.1",
    "cheerio": "0.22.0",
    "code": "3.0.2",
    "eslint-config-semistandard": "7.0.0",
    "eslint-config-standard": "6.0.1",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-standard": "2.0.0",
    "gulp": "3.9.1",
    "gulp-add-src": "0.2.0",
    "gulp-concat": "2.6.0",
    "gulp-insert": "0.5.0",
    "gulp-remote-src": "0.4.1",
    "gulp-replace": "0.5.4",
    "gulp-uglify": "2.0.0",
    "lab": "11.0.1",
    "lodash": "4.16.0",
    "nodemon": "1.10.2",
    "platform": "1.3.1",
    "prompt": "^1.0.0",
    "proxyquire": "1.4.0",
    "selenium-webdriver": "2.53.3",
    "sinon": "1.17.6",
    "snyk": "1.17.1"
  },
  "snyk": true
}
