    "visionary": "6.0.1",
    "yar": "8.1.0"
  },
  "devDependencies": {
    "benchmark": "2.1.2",
    "cheerio": "0.22.0",
    "code": "3.0.2",
    "eslint-config-semistandard": "7.0.0",
    "eslint-config-standard": "6.1.0",
