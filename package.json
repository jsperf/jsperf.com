{
  "name": "jsperf.com",
  "version": "2.0.0",
  "description": "jsPerf aims to provide an easy way to create and share test cases, comparing the performance of different JavaScript snippets by running benchmarks",
  "engines": {
    "node": "8.x.x"
  },
  "scripts": {
    "build": "gulp",
    "lint": "lab -d -L",
    "start": "node server.js",
    "test": "lab -I __core-js_shared__,__grim__ -t 100 -c -L -r lcov -o lcov.info -r console -o stdout -r html -o coverage.html test/unit",
    "test-e2e": "lab -L -m 30000 -M 10000 test/e2e",
    "snyk-protect": "snyk protect",
    "deploy": "now",
    "watch": "nodemon -e js,hbs server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jsperf/jsperf.com.git"
  },
  "keywords": [
    "jsperf"
  ],
  "contributors": [
    "Max Beatty"
  ],
  "bugs": {
    "url": "https://github.com/jsperf/jsperf.com/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/jsperf/jsperf.com",
  "dependencies": {
    "bell": "^8.8.0",
    "blipp": "3.0.0",
    "boom": "^6.0.0",
    "catbox": "^9.0.0",
    "catbox-redis": "^4.0.0",
    "confidence": "3.0.2",
    "dotenv": "^4.0.0",
    "glue": "^5.0.0",
    "good": "^8.0.0",
    "good-console": "^6.4.0",
    "good-loggly": "3.1.0",
    "good-squeeze": "^5.0.1",
    "handlebars": "^4.0.6",
    "hapi": "^17.0.0",
    "hapi-auth-cookie": "8.0.0",
    "hapi-qs": "^1.1.2",
    "highlight.js": "^9.8.0",
    "hoek": "^5.0.0",
    "inert": "^5.0.0",
    "joi": "^13.0.0",
    "jsesc": "^2.3.0",
    "lodash.assign": "4.2.0",
    "lodash.omit": "4.5.0",
    "marky-markdown": "^11.3.2",
    "mysql": "^2.15.0",
    "scooter": "4.0.0",
    "umzug": "2.0.1",
    "vision": "^5.0.0",
    "visionary": "^6.0.2",
    "yar": "^9.0.0"
  },
  "devDependencies": {
    "benchmark": "2.1.4",
    "cheerio": "0.22.0",
    "code": "^5.1.0",
    "eslint-config-semistandard": "11.0.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "gulp": "3.9.1",
    "gulp-add-src": "0.2.0",
    "gulp-clean-css": "^3.9.0",
    "gulp-concat": "^2.6.1",
    "gulp-insert": "0.5.0",
    "gulp-remote-src": "^0.4.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "0.6.1",
    "gulp-uglify": "^3.0.0",
    "lab": "^14.3.1",
    "lodash": "^4.16.6",
    "nodemon": "^1.12.1",
    "platform": "^1.3.2",
    "prompt": "^1.0.0",
    "proxyquire": "1.8.0",
    "saucelabs": "^1.3.0",
    "selenium-webdriver": "^3.6.0",
    "sinon": "^4.0.1",
    "snyk": "^1.42.7"
  },
  "snyk": true,
  "now": {
    "alias": "jsperf.com",
    "env": {
      "NODE_ENV": "production",
      "SCHEME": "https",
      "DOMAIN": "jsperf.com",
      "GITHUB_CLIENT_ID": "@jsperf-github-id",
      "GITHUB_CLIENT_SECRET": "@jsperf-github-secret",
      "GITHUB_CALLBACK": "https://jsperf.com",
      "BROWSERSCOPE": "@jsperf-browserscope",
      "BELL_COOKIE_PASS": "@jsperf-bell-pass",
      "COOKIE_PASS": "@jsperf-cookie-pass",
      "MYSQL_USER": "@jsperf-db-user",
      "MYSQL_PASSWORD": "@jsperf-db-pass",
      "MYSQL_DATABASE": "@jsperf-db-db",
      "MYSQL_HOST": "@jsperf-db-host",
      "MYSQL_PORT": "@jsperf-db-port",
      "LOGGLY_TOKEN": "@jsperf-loggly-token",
      "LOGGLY_SUBDOMAIN": "@jsperf-loggly-subdomain",
      "REDIS_HOST": "@jsperf-redis-host",
      "REDIS_PORT": "@jsperf-redis-port",
      "REDIS_PASSWORD": "@jsperf-redis-password"
    },
    "public": true
  }
}
